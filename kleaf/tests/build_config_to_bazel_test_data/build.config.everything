DEVICE_KERNEL_DIR=${KERNEL_DIR}
KERNEL_DIR=common

. ${ROOT_DIR}/${KERNEL_DIR}/build.config.common
. ${ROOT_DIR}/${KERNEL_DIR}/build.config.gki
. ${ROOT_DIR}/${KERNEL_DIR}/build.config.aarch64

BUILD_CONFIG_FRAGMENTS=${DEVICE_KERNEL_DIR}/build.config.fragment
FAST_BUILD=1
LTO=thin
FILES="
myfile/myfile1

myfile/myfile2
"
EXT_MODULES="
${DEVICE_KERNEL_DIR}/ext_module1
${DEVICE_KERNEL_DIR}/ext_module2
"
KCONFIG_EXT_PREFIX=${DEVICE_KERNEL_DIR}
UNSTRIPPED_MODULES=*
KMI_SYMBOL_LIST=android/abi_symbollist_mydevice
ADDITIONAL_KMI_SYMBOL_LISTS="
android/abi_symbollist_additional1
android/abi_symbollist_additional2
"
TRIM_NONLISTED_KMI=1
KMI_SYMBOL_LIST_STRICT_MODE=1
ABI_DEFINITION=androidabi.xml
KMI_ENFORCED=1
KMI_SYMBOL_LIST_ADD_ONLY=1
KMI_SYMBOL_LIST_MODULE_GROUPING=1
KBUILD_SYMTYPES=1
GENERATE_VMLINUX_BTF=1
BUILD_BOOT_IMG=1
BUILD_VENDOR_BOOT_IMG=1
BUILD_DTBO_IMG=1
BUILD_VENDOR_KERNEL_BOOT=1
BUILD_INITRAMFS=1
MKBOOTIMG_PATH=mymkbootimg
MODULES_OPTIONS="
option foo param=value
option bar param=value
"

MODULES_LIST=${DEVICE_KERNEL_DIR}/modules_list
MODULES_BLOCKLIST=${DEVICE_KERNEL_DIR}/modules_blocklist
SYSTEM_DLKM_MODULES_LIST=${DEVICE_KERNEL_DIR}/system_dlkm_modules_list
SYSTEM_DLKM_MODULES_BLOCKLIST=${DEVICE_KERNEL_DIR}/system_dlkm_modules_blocklist
SYSTEM_DLKM_PROPS=${DEVICE_KERNEL_DIR}/system_dlkm_props
VENDOR_DLKM_MODULES_LIST=${DEVICE_KERNEL_DIR}/vendor_dlkm_modules_list
VENDOR_DLKM_MODULES_BLOCKLIST=${DEVICE_KERNEL_DIR}/vendor_dlkm_modules_blocklist
VENDOR_DLKM_PROPS=${DEVICE_KERNEL_DIR}/vendor_dlkm_props

GKI_BUILD_CONFIG=${KERNEL_DIR}/build.config.gki.aarch64
GKI_PREBUILTS_DIR=prebuilts/gki

DTS_EXT_DIR=${DEVICE_KERNEL_DIR}
BUILD_GKI_CERTIFICATION_TOOLS=1

UNKNOWN_BUILD_CONFIG=1
DO_NOT_STRIP_MODULES=1
